{
  "name": "texshake",
  "description": "Shake your LaTeX syntax tree!",
  "version": "2.0.1",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bin": "lib/cli.js",
  "bugs": {
    "url": "https://github.com/aureooms/texshake/issues"
  },
  "dependencies": {
    "@aureooms/js-grammar": "^5.0.0",
    "@aureooms/js-itertools": "^3.4.0",
    "@aureooms/js-stream": "^0.0.2",
    "babel-polyfill": "^6.26.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.2",
    "np": "^3.0.4",
    "nyc": "^12.0.2"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/aureooms/texshake#readme",
  "keywords": [
    "abstract",
    "ast",
    "latex",
    "parser",
    "syntax",
    "tree"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/texshake"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava ./test/src --concurrency 8"
  }
}
